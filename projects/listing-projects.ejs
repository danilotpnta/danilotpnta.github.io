                                </div>
                                
                                <!-- RIGHT Content -->
                                <div class="g-col-12 g-col-md-5">
                                    <div class="card-body post-contents links-handler">
                                        
                                        <% if (item.description) { %>
                                            <p class="p-description">
                                                <%= item.description %>
                                            </p>
                                        
                                        <% } %>

                                        <!-- <a  class="button small flex" href="<%- item.path %>" > -->
                                        <a  class="button small flex" href="#" style="pointer-events: none;">
                                            About
                                        </a> 
                                        
                                        <% if (item.demo) { %>
                                            <a  class="button small flex" href="<%- item.demo %>" target="_blank" rel="noreferrer">
