<% let array_categories = [] %>
<% let id_modals = [] %>
<div class="list grid" style="column-gap: 23px; row-gap: .9rem;">
    <% for (const item of items) { %>


        <!-- To change increase num columns modify, g-col-md-6: 2columns, g-col-md-4: 3columns, g-col-md-3: 4 columns-->
        <div class="card border-1 g-col-12 g-col-sm-6 g-col-md-4 mb-2" <%=metadataAttrs(item) %>>


            <!-- Starting of MODAL -->
